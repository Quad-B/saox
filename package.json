{
  "name": "saox",
  "version": "1.5.2",
  "description": "Sanook Music (Or JOOX) Web App",
  "main": "./index.js",
  "scripts": {
    "start": "electron .",
    "buildwin": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=img/saoxlogo.ico --prune=true --version-string.CompanyName=\"Quad B Software\" --version-string.FileDescription=SAOX --version-string.ProductName=\"SAOX\"",
    "pack": "electron-builder --dir",
    "dist-all": "electron-builder -mwl",
    "dist-win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps",
    "release": "release-it --config .release-it.json --ci --no-npm.publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyphongsakorn/saox.git"
  },
  "author": "BoyPhongsakorn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boyphongsakorn/saox/issues"
  },
  "build": {
    "appId": "com.aaronlenoir.gnucash-reporter",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },{"target": "portable"}
      ]
    }
  },
  "homepage": "https://github.com/boyphongsakorn/saox#readme",
  "dependencies": {
    "async": "^2.5.0",
    "oauth-electron-facebook": "^0.1.2",
    "run-async": "^2.3.0"
  },
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-packager": "^13.1.1",
    "jquery": "^3.4.1",
    "webpack": "^3.0.0"
  }
}
